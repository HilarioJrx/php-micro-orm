{
  "name": "byjg/micro-orm",
  "description": "A micro framework for create a very simple decoupled ORM (sqlite, mysql, postgres, sqlserver)",
  "autoload": {
    "psr-4": {
      "ByJG\\MicroOrm\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6.0",
    "ext-json": "*",
    "byjg/anydataset-db": "4.1.*",
    "byjg/serializer": "1.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*|7.4.*"
  },
  "suggest": {
    "ext-curl": "*",
    "ext-pdo_mysql": "*"
  },
  "license": "MIT"
}
